SELECT
    取引先コード,
    format(sum(金額), '#,##0') AS 売上金額,
    count(取引先コード) AS 売上伝票行数
FROM
    [Excel 8.0;DATABASE=C:\app\workspace\販売管理.xls].取引データ
GROUP BY
    取引先コード
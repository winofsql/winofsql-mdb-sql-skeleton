SELECT
    取引先コード,
    format(sum(金額), '#,##0') AS 売上金額,
    count(取引先コード) AS 売上伝票行数
FROM
    [ODBC;DSN=S3-LIGHTBOX;Database=C:\app\workspace\lightbox.sqlite3].取引データ
GROUP BY
    取引先コード
SELECT
    取引先コード,
    sum(金額) AS 売上金額
FROM
    OPENDATASOURCE('Microsoft.ACE.OLEDB.12.0','Data Source=C:\app\workspace\販売管理.accdb')...取引データ
GROUP BY
    取引先コード